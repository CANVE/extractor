    def settings: Settings = {
      val s = new scala.tools.nsc.Settings
      s.classpath.value = classPath.mkString(File.pathSeparator)
  
      val path = s"./target/scala-$ShortScalaVersion/test-generated-classes"
      new File(path).mkdirs()
      s.d.value = path
      s
    }
